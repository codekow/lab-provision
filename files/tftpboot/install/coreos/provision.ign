  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Update FCOS sources for http hosting\nWants=network-online.target\nAfter=network-online.target\n[Service]\nType=oneshot\nRemainAfterExit=true\nExecStart=/usr/local/bin/update-coreos.sh\n[Install]\nWantedBy=default.target\n",
